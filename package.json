{
  "name": "scriptin",
  "version": "2.0.0",
  "description": "Javascript and CSS loaded that uses localforage to cache scripts on browser for super-fast loads.",
  "main": "src/scriptin.js",
  "type": "module",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "script",
    "css",
    "loader",
    "cache"
  ],
  "author": "Anthony Mugendi <ngurumugz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "chalk": "^5.3.0",
    "husky": "^8.0.0",
    "prettier": "^3.2.4",
    "rollup": "^4.12.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-gzip": "^3.1.1",
    "rollup-plugin-reloadsite": "^1.0.2",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-sizes": "^1.0.6",
    "rollup-plugin-terser": "^7.0.2",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "date-fns": "^3.3.1"
  }
}
