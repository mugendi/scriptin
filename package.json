{
  "name": "scriptin",
  "version": "2.0.0",
  "description": "Javascript and CSS loaded that uses localforage to cache scripts on browser for super-fast loads.",
  "main": "src/scriptin.js",
  "type": "module",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "script",
    "css",
    "loader",
    "cache"
  ],
  "author": "Anthony Mugendi <ngurumugz@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-async-to-generator": "^7.23.3",
    "@babel/plugin-transform-destructuring": "^7.23.3",
    "@babel/plugin-transform-instanceof": "^7.23.3",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/plugin-transform-shorthand-properties": "^7.23.3",
    "@rollup-extras/plugin-clean": "^1.3.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "chalk": "^5.3.0",
    "husky": "^8.0.0",
    "prettier": "^3.2.4",
    "rollup": "^4.12.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-gzip": "^3.1.1",
    "rollup-plugin-import-css": "^3.5.0",
    "rollup-plugin-reloadsite": "^1.0.4",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-sizes": "^1.0.6",
    "rollup-plugin-terser": "^7.0.2",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "lodash.merge": "^4.6.2",
    "pretty-bytes": "^6.1.1"
  }
}
